// SN75LVDS83B
	lvds-encoder {
		compatible = "lvds-encoder";
		#address-cells = <1>;
		#size-cells = <0>;

		ports {
			#address-cells = <1>;
			#size-cells = <0>;

			port@0 {
				reg = <0>;

				lvds_enc_in: endpoint {
					remote-endpoint = <&dc_rgb_output>;
				};
			};

			port@1 {
				reg = <1>;

				lvds_enc_out: endpoint {
					remote-endpoint = <&wikipad_lcd_in>;
				};
			};
		};
	};

	wikipad_lcd: panel {
		compatible = "wikipad7,lcd", "panel-lvds";

		width-mm = <151>;
		height-mm = <94>;

		data-mapping = "jeida-18";
		//power-supply1 = <&vdd_lvds_areg>;					// Fudged driver to implement multiple rails
		//power-supply2 = <&vdd_lvds_dreg>;
		enable-gpios = <&gpio TEGRA_GPIO(N, 6) GPIO_ACTIVE_HIGH>;		// Actually implemented on the LVDS encoder

		backlight = <&lcd_backlight>;

		panel-timing {
			/* 800x1280 @60Hz */
			clock-frequency = <81750000>;
			hsync-len = <64>;
			vsync-len = <1>;
			hback-porch = <128>;
			vback-porch = <2>;
			hactive = <800>;
			vactive = <1280>;
			hfront-porch = <64>;
			vfront-porch = <5>;
		};

		port {
			wikipad_lcd_in: endpoint {
				remote-endpoint = <&lvds_enc_out>;
			};
		};
	};

